{
  "numeric_detection": true,
  "dynamic_date_formats": [
    "yyyy/MM/dd",
    "yyyy/MM/dd HH:mm:ss",
    "yyyy/MM/dd HH:mm:ss Z"
  ],
  "properties": {
    "timestamp": {
      "type": "date",
      "format": "epoch_millis"
    },
    "userId": {
      "type": "long"
    },
    "spaceId": {
      "type": "long"
    },
    "module": {
      "type": "keyword"
    },
    "subModule": {
      "type": "keyword"
    },
    "operation": {
      "type": "keyword"
    },
    "status": {
      "type": "short"
    },
    "duration": {
      "type": "long"
    },
    "errorCode": {
      "type": "long"
    },
    "errorMessage": {
      "type": "text",
      "index_options": "offsets"
    }
  }
}
