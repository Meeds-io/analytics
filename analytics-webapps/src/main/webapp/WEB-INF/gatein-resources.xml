        <module>extensionRegistry</module>
      </depends>
      <depends>
        <module>commonAnalyticsVueComponents</module>
      </depends>
      <depends>
        <module>jquery</module>
        <as>$</as>
      </depends>
      <depends>
        <module>eCharts</module>
      </depends>
    </module>
  </portlet>

